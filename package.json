    "jest": "^29.6.3",
    "jest-expect-message": "^1.1.3",
    "jest-extended": "^4.0.1",
    "jest-puppeteer": "^9.0.0",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@google-cloud/vision": "^4.0.1",
