    "gen:check-entity-types": "ts-node scripts/generateCheckEntityTypes.ts",
    "gen:format-instructions": "ts-node scripts/generateFormatInstructions.ts",
    "gen:interface-repositories": "ts-node scripts/generateInterfaceRepositories.ts",
    "gen:vector-db-repositories": "ts-node scripts/generateVectorDbRepositories.ts",
    "gen:initialize-vector-db": "ts-node scripts/generateInitializeVectorDb.ts",
    "fmt": "prettier --write \"**/*.ts\" && eslint --fix \"**/*.ts\"",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
